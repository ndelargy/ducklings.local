{
    "name":"screenswiki",
    "type":"front",
    "description":"Theme used to render /screens namespace. for creating and managing UI screens",
    "version":"0.1.0",
    "author":"Vinod Kulkarni <vinod.kulkarni@gmail.com>",
    "layouts":{
        "screen-admin":{
            "layout":{
                "template":"screen-admin.html",
                "sections":{
                    "styles":{
                        "template":"styles.html",
                        "varyParams":false
                    },
                    "scripts":{
                        "template":"scripts.html"
                    },
                    "header":{
                        "template":"header.html",
                        "cache":true,
                        "varyParams":false
                    },
                    "messages":{
                        "template":"messages.html"
                    },
                    "body":{
                        "template":"body.html"
                    }
                }
            }
        },
        "screen-view":{
            "layout":{
                "template":"screen-view.html",
                "sections":{
                    "styles":{
                        "template":"styles.html",
                        "cache":true,
                        "varyParams":false
                    },
                    "scripts":{
                        "template":"scripts.html"
                    },
                    "header":{
                        "template":"header.html",
                        "cache":true,
                        "varyParams":false
                    },
                    "messages":{
                        "template":"messages.html"
                    },
                    "body":{
                        "template":"body.html"
                    }
                }
            }
        },
        "screen-list":{
            "layout":{
                "template":"screen-list.html",
                "sections":{
                    "styles":{
                        "template":"styles.html",
                        "cache":true,
                        "varyParams":false
                    },
                    "scripts":{
                        "template":"scripts.html"
                    },
                    "header":{
                        "template":"header.html",
                        "cache":true,
                        "varyParams":false
                    },
                    "messages":{
                        "template":"messages.html"
                    },
                    "body":{
                        "template":"body.html"
                    }
                }
            }
        },
        "screen-edit":{
            "layout":{
                "template":"screen-edit.html",
                "copyFrom":"screen-admin"
            }
        },
        "screen-raw":{
            "layout":{
                "template":"screen-raw.html",
                "copyFrom":"screen-admin"
            }
        },
        "screen-default":{
            "layout":{
                "template":"screen-default.html",
                "sections":{
                    "styles":{
                        "template":"styles.html",
                        "cache":true,
                        "varyParams":false
                    },
                    "scripts":{
                        "template":"scripts.html"
                    },
                    "header":{
                        "template":"header.html",
                        "cache":true,
                        "varyParams":false
                    },
                    "messages":{
                        "template":"messages.html"
                    },
                    "body":{
                        "template":"body.html"
                    }
                }
            }
        }
    }
}
